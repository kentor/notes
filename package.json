{
  "name": "notes-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "yarn clean && yarn js:build && yarn static:build",
    "clean": "rm -fr public && mkdir -p public",
    "gen-icons": "babel-node --extensions '.ts' src/scripts/gen-icons",
    "js:build": "webpack --config webpack.prod.js",
    "js:watch": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint . --ext=.js,.ts,.tsx",
    "prestart": "yarn clean",
    "start": "npm-run-all --parallel js:watch static:watch",
    "static:build": "copy-and-watch 'src/static/**/*' public",
    "static:watch": "yarn static:build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc",
    "webserver": "live-server --entry-file=index.html --ignore=js --no-browser --port=10000 public",
    "youtube-title": "babel-node -r dotenv/config --extensions '.ts' src/scripts/youtube-title"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^25.1.4",
    "@types/marked": "^0.7.3",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-timeago": "^4.1.1",
    "@types/react-transition-group": "^4.2.4",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "copy-and-watch": "^0.1.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "firebase": "^7.10.0",
    "firebase-admin": "^8.10.0",
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.2",
    "jest": "^25.1.0",
    "live-server": "^1.2.1",
    "marked": "^0.8.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-redux": "^7.2.0",
    "react-timeago": "^4.4.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "styled-jsx": "^3.2.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
