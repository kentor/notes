{
  "name": "notes-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "clean": "rm -fr public && mkdir -p public",
    "gen-icons": "babel-node --extensions '.ts' src/scripts/gen-icons",
    "lint": "eslint . --ext=.js,.ts,.tsx",
    "prestart": "yarn clean",
    "start": "vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc",
    "webserver": "live-server --entry-file=index.html --ignore=js --no-browser --port=10000 public",
    "youtube-title": "babel-node -r dotenv/config --extensions '.ts' src/scripts/youtube-title"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^25.1.4",
    "@types/marked": "^0.7.3",
    "@types/node": "^13.9.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-timeago": "^4.1.1",
    "@types/react-transition-group": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-jest": "^25.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "firebase": "^7.10.0",
    "firebase-admin": "^8.10.0",
    "jest": "^25.1.0",
    "live-server": "^1.2.1",
    "marked": "^0.8.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-timeago": "^7.1.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "typescript": "^4.7.0",
    "vite": "^2.9.12",
    "vite-tsconfig-paths": "^3.5.0",
    "zod": "^3.17.3"
  }
}
